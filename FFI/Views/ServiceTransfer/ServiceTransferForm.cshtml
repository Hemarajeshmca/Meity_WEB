<script>
    function save() {
        debugger;
        var memid = $("#txtmember_id").val();
        var memid_1 = $("#txtmember1_id").val();

        if (memid != "" && memid != null && memid_1 != "" && memid_1 != null) {
            if ($("#txtmember_id").val() == $("#txtmember1_id").val()) {

                kendoAlert("Can't create transfer certificate request for same FPO member");
                //$('#divEdit #btnSaveEd').prop('disabled', false);
                $('#divCreate #review').prop('disabled', true);
                $('#divCreate #btnSave').prop('disabled', false);
                return false;
            }
        }
        if (memid != "" && memid != null && memid_1 != "" && memid_1 != null) {
            save_Image();

        }


    }
    function save_Image() {
        var xhr = new XMLHttpRequest();
        var fd = new FormData();
        xhr.onreadystatechange = function () {
            if (xhr.readyState == 4 && xhr.status == 200) {
                var value = JSON.parse(xhr.responseText);
                if (value.result != undefined) {
                    $("#txtImageHidden").val(value.result);
                }
                save_trans();
            }
        };

        fd.append("file", document.getElementById('farmerfile').files[0]);
        fd.append("userID", $('#txtUserId').val());
        xhr.open("POST", "/UserDefinition/User/", true);
        xhr.send(fd);
        xhr.addEventListener("load", function (event) {
        }, false);
    }

    function Singleretrieve_SerReqTransfer_details(Context) {

        debugger;
        try {
            $.ajax({
                type: "POST",
                data: JSON.stringify({ orgnId: Context.orgnId, locnId: Context.locnId, userId: Context.userId, localeId: Context.localeId, Header: Context.Header }),
                url: "/ServiceTransfer/SingleServiceList",
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    debugger;
                    if (response.context != null) {
                        generate_SerReqTran_header(response.context.Header)

                    }
                    if (response.ApplicationException != null) {
                        kendoAlert(response.ApplicationException.errorDescription);
                    }
                },
                error: function (er) {
                    alert(er)
                    console.log(er)
                }

            });
        }
        catch (err) {
            javascript_log4j_root(arguments.callee.name, err);
        }

    }
    function SingleHistory_SerReqTransferdetail_details(Context) {
        debugger;
        try {
            $.ajax({
                type: "POST",
                data: JSON.stringify({ orgnId: Context.orgnId, locnId: Context.locnId, userId: Context.userId, localeId: Context.localeId, Header: Context.Header }),
                url: "/ServiceTransfer/SingleServiceBankDetails",
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    debugger;
                    if (response.context.Header != null) {
                        generate_SerReqTran_Header(response.context.Header)

                    }
                    if (response.context.Share_certificate != null) {
                        generate_SerReqTran_share_certificate(response.context.Share_certificate)
                    }
                    if (response.context.Request_history != null) {
                        generate_SerReqTran_request_history(response.context.Request_history)
                    }
                    if (response.ApplicationException != null) {
                        kendoAlert(response.ApplicationException.errorDescription);
                    }
                },
                error: function (er) {
                    alert(er)
                    console.log(er)
                }

            });
        }
        catch (err) {
            javascript_log4j_root(arguments.callee.name, err);
        }

    }
    function save_trans() {
        $("#txtlookup").val("false");
        if ($("#txtMode").val() == "S") {
            $("#txtMode").val('U');

        }
        var formval = form_Serialize("servicetransfer");
        var obj_val = JSON.parse(formval);
        if ($("#TextArea1").val() == "" || $("#TextArea1").val() == null) {
            kendoAlert("Reason For Transfer Can't be blank");
            return;
        }
        if ($("#servicereq_rowid").val() == undefined || $("#servicereq_rowid").val() == "") {
            obj_val.IOU_servicereq_rowid = 0;
        }
        else {
            obj_val.IOU_servicereq_rowid = $("#servicereq_rowid").val();
        }
        if ($("#txtcommon_id").val() == undefined || $("#txtcommon_id").val() == "") {
            obj_val.IOU_servicereq_no = "0";
        }
        else {
            obj_val.IOU_servicereq_no = $("#txtcommon_id").val();
        }
        if (obj_val.In_status_code == undefined) {
            obj_val.In_status_code = "A";
        }
        if (obj_val.In_row_timestamp == undefined) {
            obj_val.In_row_timestamp = "0";
        }
        if ($("#chklst_status_flag").val() == undefined) {
            obj_val.In_chklist_status_flag = 0;
        }
        else {
            obj_val.In_chklist_status_flag = $("#chklst_status_flag").val();
        }
        if (obj_val.In_row_timestamp == undefined) {
            obj_val.In_row_timestamp = "0";
        }
