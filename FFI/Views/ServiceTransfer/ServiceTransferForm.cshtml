        data_save.context = getContext();
        data_save.context.Header = obj_val;
        var Context = data_save.context;

        $.ajax({
            type: "POST",
            data: JSON.stringify({ orgnId: Context.orgnId, locnId: Context.locnId, userId: Context.userId, localeId: Context.localeId, Header: Context.Header }),
            url: "/ServiceTransfer/SaveReqTransfer",
            dataType: "json",
            contentType: 'application/json; charset=utf-8',
            success: function (response) {
                debugger;
                var Res = response;
                if (Res.context != null) {
                    var msg = Res.ApplicationException;
                    var mode = $("#txtMode").val();
                    //retrive_SingleFetch_Details(Res.context);
                    if (Res != null) {
                        if (mode == "I") {
                            kendoAlert("Transfer Request Details Saved Successfully");
                            get_SerReqTran_details(Res.context.Header);
                        }
                        else if (mode == "R") {
                            kendoAlert("Transfer Request Details Send For Review");
                            get_SerReqTran_details(Res.context.Header);
                        }
                        else if (mode == "A") {
                            kendoAlert("Transfer Request Details Approved Successfully");
                            get_SerReqTran_details(Res.context.Header);
                        }
                        else if (mode == "J") {
                            kendoAlert("Transfer Request Details Rejected Successfully");
                            get_SerReqTran_details(Res.context.Header);
                        }
                        else  {
                            kendoAlert("Transfer Request Details Saved Successfully");
                            get_SerReqTran_details(Res.context.Header);
                        }
                    }
                    else {
                        kendoAlert(Res.ApplicationException.errorDescription);
                    }


                }
            },
            error: function (er) {
                alert(er)
                console.log(er)
            }

        });
        //save_SerReqTrans_details(data_save);

    }
    
