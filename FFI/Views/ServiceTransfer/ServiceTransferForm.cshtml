                    //retrieve_SerReqtrans_details(data);
                    //retrieve_SerReqTransdetail_details(data);
                    $("#txtMode").val("U");
                    $("#txtcommon_id").val(obj_val.Out_servicereq_no);
                    setlocalStorage('doc_no', $("#txtcommon_id").val());
                    setlocalStorage('doc_row_id', $("#txtservicereq_rowid").val())
                }
            }
        }

        function field_enable() {
            if (permission.auth == true || getlocalStorage('btn_clk_val') == "View") {
                $("#service_req_name :input").attr("readonly", "");
                $("#pay_help1").hide();
                $("#txtComments").removeAttr("readonly");
