<script>
    function save() {
        debugger;
        try {
            if ($('#txtmember_id').val() == '' || $('#txtmember_id').val() == undefined) {
                kendoAlert('Please Select the Member ID');
            }
            else {
                //if ($("#txtmode").val() == "S") {
                //    $("#txtmode").val('U');
                //}
                var formval = form_Serialize("service_req_name");
                var obj_val = JSON.parse(formval);
                if ($("#servicereq_rowid").val() == undefined || $("#servicereq_rowid").val() == "") {
                    obj_val.IOU_servicereq_rowid = 0;
                }
                else {
                    obj_val.IOU_servicereq_rowid = $("#servicereq_rowid").val();
                }
                if ($("#txtcommon_id").val() == undefined || $("#txtcommon_id").val() == "") {
                    obj_val.IOU_servicereq_no = "0";
                }
                else {
                    obj_val.IOU_servicereq_no = $("#txtcommon_id").val();
                }
                if (obj_val.status_code == undefined) {
                    obj_val.In_status_code = "A";
                }
                if (obj_val.row_timestamp == undefined) {
                    obj_val.In_row_timestamp = "0";
                }
                if ($("#chklst_status_flag").val() == undefined) {
                    obj_val.In_chklst_status_flag = 0;
                }
                else {
                    obj_val.In_chklst_status_flag = $("#chklst_status_flag").val();
                }
                if (obj_val.row_timestamp == undefined) {
                    obj_val.In_row_timestamp = "0";
                }
                if ($("#txtComments").val() == undefined || $("#txtComments").val() == "") {
                    obj_val.In_sr_comments = "";
                }
                else {
                    obj_val.In_sr_comments = $("#txtComments").val();
                }


                obj_val.In_farmer_code = obj_val.farmer_code;
                obj_val.In_fpomember_code = obj_val.fpomember_code;
                obj_val.In_changeto_farmer_name = obj_val.changeto_farmer_name;
                obj_val.In_changeto_sur_name = obj_val.changeto_sur_name;
                obj_val.In_cur_farmer_name = obj_val.cur_farmer_name;
                obj_val.In_cur_sur_name = obj_val.cur_sur_name;
                obj_val.In_mode_flag = obj_val.mode_flag;


                var data_save = {};
                data_save.context = WebAPIProxy.getContext();
                data_save.context.Header = obj_val;
                var Context = data_save.context;

                $.ajax({
                    type: "POST",
                    data: JSON.stringify({ orgnId: Context.orgnId, locnId: Context.locnId, userId: Context.userId, localeId: Context.localeId, Header: Context.Header }),
                    url: "/Service/SaveReqName",
                    dataType: "json",
                    contentType: 'application/json; charset=utf-8',
                    success: function (response1) {
                        var Res = response1;
                        if (Res != null) {
                            var mode = $("#txtMode").val();
                            if (mode == "I") {
                                kendoAlert("Name Change Details saved successfully");
                                get_SerReqName_details(Res.context.Header);
                            }
                            else if (mode == "D") {
                                kendoAlert("Name Change Details inactivate successfully");
                                get_SerReqName_details(Res.context.Header);
                            }
                            else if (mode == "R") {
                                kendoAlert("Name Change Details Send For Review");
                                get_SerReqName_details(Res.context.Header);
                            }
                            else if (mode == "A") {
                                kendoAlert("Name Change Details Approved successfully");
                                get_SerReqName_details(Res.context.Header);
                            }
                            else if (mode == "J") {
                                kendoAlert("Name Change Details Rejected successfully");
                                get_SerReqName_details(Res.context.Header);
                            }
                            else {
                                kendoAlert("Name change Saved Successfully");
                                get_SerReqName_details(Res.context.Header);
                                return false;
                            }
                        }
                        else {
                            kendoAlert(Res.ApplicationException.errorDescription);
                        }
                    },
                    error: function (er) {
                        alert(er)
                        console.log(er)
                    }

                });

                //save_SerReqName_details(data_save);
            }
        }
        catch (err) {
            javascript_log4j_root(arguments.callee.name, err);
        }
    }
