<script>
    var permission = "";
    $(document).ready(function () {
        debugger;
        $("#lblForm_menuid").text("SRVNAME");
        kendodate_format();
        //master_service();
        Form_validate_Name = $('form.form-horizontal').attr('id');
        var container = $("#service_req_name");
        kendo.init(container);
        container.kendoValidator({
        });

        $(document).on('click', '#btnSave', function (e) {

            var Given_name = $('#txtgiven_name').val();
            var sur_name = $('#txtsurname').val();
            if (Given_name != "" && Given_name != null && sur_name != "" && sur_name != null) {
                $('#divCreate #btnSave').prop('disabled', true);
                $('#divCreate #review').prop('disabled', false);
            }
            else {
                $('#divCreate #btnSave').prop('disabled', false);
                $('#divCreate #review').prop('disabled', true);
            }
        });
        $(document).on('click', '#btnSaveEd', function (e) {

            var Given_name = $('#txtgiven_name').val();
            var sur_name = $('#txtsurname').val();
            if (Given_name != "" && Given_name != null && sur_name != "" && sur_name != null) {
                $('#divEdit #btnSaveEd').prop('disabled', true);
                $('#divEdit #reviewEd').prop('disabled', false);
            }
            else {
                $('#divEdit #btnSaveEd').prop('disabled', false);
                $('#divEdit #reviewEd').prop('disabled', false);
            }
        });
        $(document).on('click', '#btnApprove', function (e) {
            $('#divEdit #btnApprove').prop('disabled', true);
            $('#divEdit #btnReject').prop('disabled', true);
        });
        $(document).on('click', '#btnReject', function (e) {
            var cmt = $('#txtComments').val();
            if (cmt != "" && cmt != null) {

                $('#divEdit #btnApprove').prop('disabled', true);
                $('#divEdit #btnReject').prop('disabled', true);
            }
        });
        $(document).on('click', '#reviewEd', function (e) {
            master_review();
            $('#divEdit #reviewEd').prop('disabled', true);
        });
        $(document).on('click', '#review', function (e) {
            master_review();
            $('#divCreate #review').prop('disabled', true);
        });




        Screen_Id = "SRVNAME";
        permission = sec_Permission(Screen_Id);
        grid_share_cert();
        grid_req_history();
        $("#common_id").text("Service Request No");
        $(".file_attach").attr("hdrtitle", "Service Request-Name Change");
        $(".notes").attr("hdrtitle", "Service Request-Name Change");

        var org = getlocalStorage('org_id');
        $('#txtaggrorgn_code').val(org);
        if (getlocalStorage('btn_clk_val') == "Create") {
            $('#divCreate #review').show();
            $('#divCreate #review').prop('disabled', true);
            $("#txtmode").val("I");
            $("#bottom_menus").show();
            $("#DocStat").val("New");
        }
        else if (getlocalStorage('btn_clk_val') == "View" || getlocalStorage('btn_clk_val') == "Edit") {
            field_enable();
            $("#divEdit #reviewEd").prop("disabled", true);
            if (getlocalStorage("ls_pageList") != undefined) {
                var data = getlocalStorage("ls_pageList");
                var formval = form_Serialize("service_req_name");
                var obj_val = JSON.parse(formval);
                if (obj_val != undefined) {
                    obj_val.IOU_fpomember_code = data.Out_fpomember_code;
                    obj_val.IOU_request_type = data.Out_request_type;
                    obj_val.IOU_servicereq_no = data.Out_servicereq_no;
                    obj_val.IOU_servicereq_rowid = data.Out_servicereq_rowid;
                    //$("#txtcommon_id").val(data.servicereq_no);
                    var data = {};
                    data.context = WebAPIProxy.getContext();
                    data.context.Header = obj_val;
                    var Context = data.context;
                    //retrieve_SerReqName_details(data);
                    retrive_SingleFetch_Details(Context);
                    //retrieve_SerReqNamedetail_details(data);
                    retrive_SingleFetchNamedetail_Details(Context);
                    $("#txtmode").val("U");
                    $("#txtcommon_id").val(obj_val.servicereq_no);
                    setlocalStorage('doc_no', $("#txtcommon_id").val());
                    setlocalStorage('doc_row_id', $("#servicereq_rowid").val());
                }
            }
        }

        function field_enable() {
            if (permission.auth == true || getlocalStorage('btn_clk_val') == "View") {
                $("#service_req_name :input").attr("readonly", "");
                $("#pay_help1").hide();
                $("#txtComments").removeAttr("readonly");
                $("#farmerfile").removeAttr("readonly");
                $("#farmerfile").prop("disabled", true);

            }
        }

        $('#HelpModal').on('hide.bs.modal', function (e) {
            if ($("#txtmember_id").val() != undefined && $("#txtmember_id").val() != "") {
                fetch_name();
            }
        });

    });

    function retrive_SingleFetch_Details(Context) {
        debugger;
        try {
            $.ajax({
                type: "POST",
                data: JSON.stringify({ orgnId: Context.orgnId, locnId: Context.locnId, userId: Context.userId, localeId: Context.localeId, Header: Context.Header }),
                url: "/Service/SingleServiceList",
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    debugger;
                    if (response.context != null) {
                        generate_SerReqName_header(response.context.Header)

                    }
                    if (response.ApplicationException != null) {
                        kendoAlert(response.ApplicationException.errorDescription);
                    }
                },
                error: function (er) {
                    alert(er)
                    console.log(er)
                }

            });
        }
        catch (err) {
            javascript_log4j_root(arguments.callee.name, err);
        }

    }
    function retrive_SingleFetchNamedetail_Details(Context) {
        debugger;
        try {
            $.ajax({
                type: "POST",
                data: JSON.stringify({ orgnId: Context.orgnId, locnId: Context.locnId, userId: Context.userId, localeId: Context.localeId, Header: Context.Header }),
                url: "/Service/SingleServiceNameDetails",
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    debugger;
                    if (response.context != null) {
                        if (response.context.Header != null) {
                            generate_SerReqName_Header(response.context.Header)

                        }
                        if (response.context.Share_certificate != null) {
                            generate_SerReqName_share_certificate(response.context.Share_certificate)
                        }
                        if (response.context.Request_history != null) {
                            generate_SerReqName_request_history(response.context.Request_history)
                        }
                        if (response.ApplicationException != null) {
                            kendoAlert(response.ApplicationException.errorDescription);
                        }
                    }
                },
                error: function (er) {
                    alert(er)
                    console.log(er)
                }

            });
        }
        catch (err) {
            javascript_log4j_root(arguments.callee.name, err);
        }

    }


</script>

