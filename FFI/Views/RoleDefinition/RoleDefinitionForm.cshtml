            navigatable: true,
            columns: [
                {
                    field: "out_orgn_level",
                    title: "Role Id",
                    hidden: true,
                    width: 30
                },
                {
                    field: "out_orgn_level_desc",
                    title: "Org Level Desc",
                    hidden: true,
                    width: 30
                },
                {
                    field: "out_role_code",
                    title: "Role Code",
                    width: 30
                },
                {
                    field: "out_role_name",
                    title: "Role Name",
                    width: 30
                },
                {
                    field: "out_role_rowid",
                    title: "Role RowId",
                    hidden: true,
                    width: 30
                },
                {
                    field: "out_status_code",
