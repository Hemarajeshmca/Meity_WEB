    function save() {
        try {
            debugger;
            var valid = save_validate();
            if (valid == "") {
                if ($("#txt_supplier_rowid").val() == "") {
                    $("#txt_supplier_rowid").val(0);
                }
                if ($("#txt_version_no").val() == "") {
                    $("#txt_version_no").val(1);
                }

                if ($("#txtMode").val() == "S" || $("#txtMode").val() == "U") {
                    $("#txtMode").val('U');
                    $('#divEdit #btnSaveEd').prop('disabled', true);
                }
                else {
                    $('#divCreate #btnSave').prop('disabled', true);
                }
                var formval = form_Serialize("ICD_Supmst");
                var obj_val = JSON.parse(formval);
                if (obj_val.In_row_timestamp == null || obj_val.In_row_timestamp == "" || obj_val.In_row_timestamp == undefined) {
                    obj_val.In_row_timestamp = "2000-01-01";
                }
                //address

                var grid_val = JSON.stringify($("#grid_Address").data().kendoGrid._data);
                var Bind = {};
                Bind = grid_val;
                var result = JSON.parse(Bind);

                $.each(result, function (index, value) {
                    debugger;
                    var row_id = value.In_supplier_addr_rowid;
                    if (row_id == undefined) {
                        result[index].In_supplier_addr_rowid = 0;
                    }
                    else {
                        result[index].In_supplier_addr_rowid = row_id;
                    }
                    var mmode = result[index].In_mode_flag;
                    if (mmode == "S") {
                        result[index].In_mode_flag = "U";
                    }

                    var Status = result[index].In_status_code;
                    if (Status == undefined || Status == "") {
                        result[index].In_status_code = "A";
                    }

                });
                var obj_grid_val = result;


                //tax
                var grid_val5 = JSON.stringify($("#grid_tax").data().kendoGrid._data);
                var Bind5 = {};
                Bind5 = grid_val5;
                var result5 = JSON.parse(Bind5);
                
                if (result5.length > 0) {
                    $.each(result5, function (index, value) {
 
                        var tax_rowid = value.In_tax_rowid;
                        if (tax_rowid == undefined || tax_rowid == '') {
                            result5[index].In_tax_rowid = 0;
                        }
                        var state_code = value.In_state_code;
                        var state_code = result5[index].In_state_code;
                        if (state_code == '' || tax_rowid == undefined) {
                            result5[index].In_state_code = "tn";
                        }
                        var mmode = result5[index].In_mode_flag;
                        if (mmode == "S") {
                            result5[index].In_mode_flag = "U";
                        }
                    });
                    var obj_grid_val5 = result5;
                }
                else {
                    var obj_grid_val5 = null;
                }
                var org = ""
                if ($("#txtic_code").val() != "") {
                    org = $("#txtic_code").val();
                } else {
                    org = $("#cmb_filter").val();
                }
                var data = {};
                debugger;
                data.context = getContext();
                data.context.orgnId = org;
                data.context.header = obj_val;
                data.context.address = obj_grid_val;
                data.context.tax = obj_grid_val5;
                var Context = data.context;
                var SupplierAddress = data.context.address;
                var SupplierTax = data.context.tax;
                $.ajax({
                    type: "POST",
                    data: JSON.stringify({ userId: Context.userId, orgnId: Context.orgnId, locnId: Context.locnId, localeId: Context.localeId, header: Context.header, SupplierAddress: Context.address, SupplierTax: Context.tax }),
                    url: "/Supplier/SupplierSave",
                    dataType: "json",
                    contentType: 'application/json; charset=utf-8',
                    success: function (response) {
                        console.log(response)
                        var Res = JSON.parse(response);
                        debugger;
                        var mode = $("#txtMode").val();
                        if (Res.applicationException.errorDescription != null && Res.applicationException.errorDescription != "") { //ramya
                            kendoAlert(Res.applicationException.errorDescription);
                            if (mode == "S" || mode == "U") {
                                $('#divEdit #btnSaveEd').prop('disabled', false);
                            }
                            else {
                                $('#divCreate #btnSave').prop('disabled', false);
                            }
                            return false;
                        }
                        if (Res.context != null) {
                            
                            if (mode == "I") {
                                kendoAlert("Supplier Master saved successfully");
                                get_Supplier_details(Res.context.Header);
                            }
                            else if (mode == "D") {
                                kendoAlert("Supplier Master inactivate successfully");
                                get_Supplier_details(Res.context.Header);

                            }
                            else {
                                kendoAlert("Supplier Master Updated successfully");
                                get_Supplier_details(Res.context.Header);
                            }
                        }
                        else {
                            kendoAlert(Res.applicationException.errorDescription);
                        }
                    },
                    error: function (er) {
                        alert(er)
                        console.log(er)
                    }

                });
            }
            else {
                kendoAlert(valid);
            }
        }
        catch (err) {
            javascript_log4j_root(arguments.callee.name, err);
        }
    }
    function get_Supplier_details(res) {
        try {
            debugger;
            var formval = form_Serialize("ICD_Supmst");
            var obj_val = JSON.parse(formval);
            //obj_val.In_supplier_rowid = data.Out_supplier_rowid; //ramya
            //obj_val.In_supplier_code = data.Out_supplier_code;
            //obj_val.In_version_no = data.Out_version_no;
            var data = {};
            //data.context = WebAPIProxy.getContext();;
            data.context = getContext();;
            data.context.header = obj_val;
            var Context = data.context;

            $.ajax({
                type: "POST",
                data: JSON.stringify({ userId: Context.userId, orgnId: Context.orgnId, locnId: Context.locnId, localeId: Context.localeId, supplier_rowid: Context.header.In_supplier_rowid, supplier_code: Context.header.In_supplier_code }),
                url: "/Supplier/Supplierfetch",
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    debugger;
