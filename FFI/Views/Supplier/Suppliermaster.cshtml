<script>
    $(document).ready(function () {
        Form_validate_Name = $('form.form-horizontal').attr('id');
        var container = $("#ICD_Supmst");
        kendo.init(container);
        container.kendoValidator({
        });
        grid_Address_details([]);
        grid_taxdetail([]);
        $(".file_attach").attr("hdrtitle", "FPO Registration");
        $(".notes").attr("hdrtitle", "FPO Registration");
        $("#lblForm_menuid").text("ICDSUPMST");
        $("#hide_div").hide();
        Screen_Id = "ICDSUPMST";
        permission = sec_Permission(Screen_Id);

        $("div#tax").hide();

        setlocalStorage("status", "Active");
        setlocalStorage('status_code', "A");

        var org = getlocalStorage('org_id');
        $('#txtagg_code').val(org);

        setlocalStorage("status", "Active");
        setlocalStorage('status_code', "A");

        if (getlocalStorage('btn_clk_val') == "Create") {
            $('#divCreate #review').hide();
            $("#txtMode").val("I");
            $("#bottom_menus").hide();
            $("#DocStat").val("New");
            load_master_code();
        }
        else if (getlocalStorage('btn_clk_val') == "View" || getlocalStorage('btn_clk_val') == "Edit") {
            debugger;
            $('#divEdit #reviewEd').hide();
            $('#divEdit #deactivate').hide();
            $('#bottom_menus').hide();
            $("#DocStat").val(getlocalStorage('btn_clk_val'));
            if (getlocalStorage("ls_pageList") != undefined) {
                var data = getlocalStorage("ls_pageList");
                var formval = form_Serialize("ICD_Supmst");
                var obj_val = JSON.parse(formval);
                obj_val.In_supplier_rowid = data.Out_supplier_rowid;
                obj_val.In_supplier_code = data.Out_supplier_code;
                obj_val.In_version_no = data.Out_version_no;
                var data = {};
                debugger;
                data.context = WebAPIProxy.getContext();;
                data.context.header = obj_val;
                var Context = data.context;
                $.ajax({
                    type: "POST",
                    data: JSON.stringify({ userId: Context.userId, orgnId: Context.orgnId, locnId: Context.locnId, localeId: Context.localeId, supplier_rowid: Context.header.In_supplier_rowid, supplier_code: Context.header.In_supplier_code }),
                    url: "/Supplier/Supplierfetch",
                    dataType: "json",
                    contentType: 'application/json; charset=utf-8',
                    success: function (response) {
                        debugger;
                        if (response.context != null) {
                            generate_Supplier_header(response.context.Header);
                            generate_Supplier_Address(response.context.SupplierAddress);
                            generate_Supplier_tax(response.context.SupplierTax);
                        }
                        else {
                            console.log("Issue where Dropped");
                        }
                    },
                    error: function (er) {
                        console.log(er);
                    }
                });
                $("#txtMode").val("U");
            }
        }
        $('#txt_pan_no').change(function (event) {
            debugger;
            var regExp = /[a-zA-z]{5}\d{4}[a-zA-Z]{1}/;
            var txtpan = $(this).val();
            $('#txt_pan_no').val(txtpan.toUpperCase());
            if (txtpan.length == 10) {
                if (txtpan.match(regExp)) {
                    $('#txt_pan_no').val(txtpan.toUpperCase());
                }
                else {
                    kendoAlert("Not a valid PAN number");
                    $('#txt_pan_no').val("");
                    return false;
                }
            }
            else {
                kendoAlert("Please enter 10 digits for a valid PAN number");
                return false;
            }

        });
        $('#txtsup_code').keypress(function (e) {
            var regex = new RegExp(/^[a-zA-Z0-9\s]+$/);
            var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
            if (regex.test(str)) {
                return true;
            }
            else {
                e.preventDefault();
                return false;
            }
        });
        $('#txtsupplier_name').keypress(function (e) {
            var regex = new RegExp(/^[a-zA-Z0-9\s]+$/);
            var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
            if (regex.test(str)) {
                return true;
            }
            else {
                e.preventDefault();
                return false;
            }
        });
     var In_agg_code = $("#txtagg_code").val();
        var data = { In_agg_code: In_agg_code, module: "ICD" };
        $.ajax({
          url: '@Url.Action("lcdcenter_search", "Product")',
         type: "post",
        contentType: 'application/x-www-form-urlencoded',
            data: data,
            success: function (response) {
                debugger;
                if (response != null) {
                    debugger;
                    var itemArr = [];
                    for (var i = 0; i < response.length; i++) {
                        var list = {};
                        list.id = response[i].Out_orgn_rowid
                        list.code = response[i].Out_orgn_code;
                        list.desc = response[i].Out_orgn_desc;
                        itemArr.push(list);
                    }
                    filter_combobox("cmb_filter", itemArr);

                }
                else {
                    $("#cmb_filter").empty();
                }
            },
            error: function (er) {
                alert(er);
                console.log(er);
            }
        });
    });
</script>
