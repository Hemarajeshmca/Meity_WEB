            if (value.In_supplier_country_desc == "" || value.In_supplier_country_desc == undefined) {
                validate_msg += "Country cannot be blank <br>";
            }
            if (value.In_supplier_state_desc == "" || value.In_supplier_state_desc == undefined) {
                validate_msg += "State cannot be blank <br>";
            }
            if (value.In_supplier_dist_desc == "" || value.In_supplier_dist_desc == undefined) {
                validate_msg += "District cannot be blank <br>";
            }
            // AS Per demo sign on 02-02-2021
            //if (value.In_supplier_panchayat_desc == "" || value.In_supplier_panchayat_desc == undefined) {
            //    validate_msg += "Panchayat cannot be blank <br>";
            //}
            //if (value.In_supplier_taluk_desc == "" || value.In_supplier_taluk_desc == undefined) {
            //    validate_msg += "Taluk cannot be blank <br>";
            //}
            //if (value.In_supplier_village_desc == "" || value.In_supplier_village_desc == undefined) {
            //    validate_msg += "Village cannot be blank <br>";
            //}

            if (value.In_supplier_addr1 == "" || value.In_supplier_addr1 == undefined) {
                validate_msg += "Supplier Address 1 cannot be blank <br>";
            }
            if (value.In_supplier_addr2 == "" || value.In_supplier_addr2 == undefined) {
                validate_msg += "Supplier Address 2 cannot be blank <br>";
            }
