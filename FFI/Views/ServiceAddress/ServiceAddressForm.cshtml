<script>
    function save() {
        debugger;

        if ($("#txtmember_id").val() == undefined || $("#txtmember_id").val() == "") {
            kendoAlert("FPO member Details needs to be Selected");
            return false;
        }
        try {

            if ($("#txtMode").val() == "S") {
                $("#txtMode").val('U');
            }
            var formval = form_Serialize("frm_serviceaddress");
            var obj_val = JSON.parse(formval);
            if ($("#servicereq_rowid").val() == undefined || $("#servicereq_rowid").val() == "") {
                obj_val.IOU_servicereq_rowid = 0;
            }
            else {
                obj_val.IOU_servicereq_rowid = $("#servicereq_rowid").val();
            }
            if ($("#txtcommon_id").val() == undefined || $("#txtcommon_id").val() == "") {
                obj_val.IOU_servicereq_no = "0";
            }
            else {
                obj_val.IOU_servicereq_no = $("#txtcommon_id").val();
            }
            if (obj_val.In_status_code == undefined) {
                obj_val.In_status_code = "A";
            }
            if (obj_val.In_row_timestamp == undefined) {
                obj_val.In_row_timestamp = "0";
            }
            if ($("#chklst_status_flag").val() == undefined) {
                obj_val.In_chklist_status_flag = "0";
            }
            else {
                obj_val.In_chklist_status_flag = $("#chklst_status_flag").val();
            }
            if (obj_val.In_row_timestamp == undefined) {
                obj_val.In_row_timestamp = "0";
            }
            if ($("#txtComments").val() == undefined || $("#txtComments").val() == "") {
                obj_val.In_sr_comments = "";
            }
            else {
                obj_val.In_sr_comments = $("#txtComments").val();
            }

            var data_save = {};
            data_save.context = WebAPIProxy.getContext();
            data_save.context.Header = obj_val;
            var Context = data_save.context;

            $.ajax({
                type: "POST",
                data: JSON.stringify({ orgnId: Context.orgnId, locnId: Context.locnId, userId: Context.userId, localeId: Context.localeId, Header: Context.Header }),
                url: "/ServiceAddress/SaveReqAddress",
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    debugger;
                    var Res = response;
                    if (Res != null) {
                        var mode = $("#txtMode").val();
                        if (mode == "I") {
                            kendoAlert("Service Request Address Details saved successfully");
                            get_SerReqaddress_details(Res.context.Header);
                        }
                        else if (mode == "D") {
                            kendoAlert("Service Request Address inactivate successfully");
                            get_SerReqaddress_details(Res.context.Header);
                        }
                        else if (mode == "R") {
                            kendoAlert("Service Request Address Send For Review");
                            get_SerReqaddress_details(Res.context.Header);
                        }
                        else if (mode == "A") {
                            kendoAlert("Service Request Address Approved successfully");
                            get_SerReqaddress_details(Res.context.Header);
                        }
                        else if (mode == "J") {
                            kendoAlert("Service Request Address Rejected successfully");
                            get_SerReqaddress_details(Res.context.Header);
                        }
                        else {
                            kendoAlert("Service Request Address saved successfully");
                            get_SerReqaddress_details(Res.context.Header);
                        }
                    }
                        else {
                            kendoAlert(Res.ApplicationException.errorDescription);
                        }
                },
                error: function (er) {
                    alert(er)
                    console.log(er)
                }

            });
            //save_SerReqAdd_details(data_save);

        }
        catch (err) {
            javascript_log4j_root(arguments.callee.name, err);
        }
    }



